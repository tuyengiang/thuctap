.paging{width:100%; text-align:center;}
.paging ul{padding:0;margin:0;list-style:none;}
.paging ul li{display:inline-block;margin:5px 2px; font:11px/24px Roboto}
.paging ul li:hover a, .paging ul li.active a{background:#d14011;width:24px; height:24px; border-radius:24px; color:#fff}
.paging ul li a{display:inline-block;width:24px; height:24px; border-radius:24px; background:#cecece; color:#333}
.paging ul li a .fa{font-size:9px;height:24px;vertical-align:middle;line-height:24px; font-weight:normal}

._RadAjax .raDiv{
    background-image:url('/images/loading.gif');
    position: relative;
    z-index: 2;
    background-color: transparent;
    background-position: center center;
    background-repeat: no-repeat;

}
._RadAjax .raDiv, .RadAjax .raColor {
    width: 100%;
    height: 100%;
    margin: 0;
    padding: 0;
}
._RadAjax .raColor {
    position: absolute;
    top: 0;
    left: 0;
    z-index: 1;
    background-image: none;
    background-color: #e6e6e6; 
    color: #767676;
}
._RadAjax .raTransp
{
    opacity:.6;-moz-opacity:.6;filter:alpha(opacity=60);
    zoom: 1;
}

