/* Default
----------------------------------------------------------*/
*
{
    margin: 0;
    padding: 0;
    outline: medium none;
}
img
{
    border: none;
    vertical-align: middle;
}
ul.ls
{
    list-style: none;
    padding: 0;
    margin: 0;
}
.footer-social li {
	float:left;
}


ul.ls a, a
{
    text-decoration: none;
}
a:hover
{
    text-decoration: underline;
}
body
{
    background-color: #ffffff;
    font: 12px Arial;
}
body input
{
    font: 12px Arial;
}
.cl
{
    clear: both;
}
.space
{
    background-color: #ffffff;
    width: 100%;
    clear: both;
    height: 2px;
}
.border-line
{
    background-color: #b7b7b7;
    width: 100%;
    clear: both;
    height: 1px;
}
/* Header
----------------------------------------------------------*/
.header
{
    position: relative;
   
    background: url("../images/header-bg.png") repeat-x scroll center transparent;
    width: 1200px;
    height: 108px;
    margin: 0 auto;
}
.header-left a
{
    float: left;
    height: 108px;
    width: 380px;
    cursor: pointer;
}
.header-center
{
    float: left;
    height: 108px;
    width: 320px;
}
.header-center .city-box {
	margin: 5px 0 0 0;
}
.header-center .city-box select {
    font-size: 16px;
    line-height: 20px;
    font-weight: normal;
	padding: 0px 20px;
    background: #555555;
    color: #fff;
}
.header-center .city-box button {
font-size: 15px;
    line-height: 20px;
    font-weight: normal;
    background-color: #555555;
    padding: 0px 10px;
    color: #fff;
    text-decoration: none;
    float: right;
    cursor: pointer;
    display: inline-block;
    border: solid 1px darkgrey;
}
.search-box
{
    position: relative;
    margin: 20px 0 0 0;
    background: url("../images/search-bg.png") no-repeat scroll center transparent;
    width: 320px;
    height: 34px;
}
.search-box #keyword
{
    float: left;
    padding: 0 4px 0 5px;
    background: none;
    border: none;
    font: 12px/34px Arial;
    height: 34px;
    width: 279px;
}
.search-box #search
{
    background: none;
    border: none;
    width: 32px;
    height: 34px;
    cursor: pointer;
}
.search-most span
{
    font: 11px/31px Arial;
    color: #ffffff;
    padding: 0 0 0 0;
}
.search-most span.title
{
    padding: 0 4px;
    color: #ffdd00;
}
.search-most
{
    float: left;
    font: italic 11px/31px Arial;
}
.search-most a
{
    color: #ffffff;
}
.header-right
{
    float: right;
    background: url("../images/header-right-bg.png") no-repeat scroll right top transparent;
    width: 500px;
    height: 88px;
}
.header-right ul.mm-info
{
    float: right;
    padding: 0 17px 0 0;
    height: 34px;
    overflow: hidden;
}
.header-right ul.mm-info li
{
    float: left;
    margin: 0 12px;
    font: 12px/34px Arial;
    color: #FFFFFF;
}
.header-right ul.mm-info li h2
{
    font: 12px/34px Arial;
}
.header-right ul.mm-info li a
{
    font: bold 12px/34px Arial;
    color: #f8f400;
    white-space: nowrap;
}
.header-right ul.mm-info li a:hover
{
    text-decoration: underline;
}
.header-right ul.mm-service
{
    float: right;
    padding: 0 15px 0 0;
    height: 70px;
    margin: 0 0 0;
}
.header-right ul.mm-service li
{
    float: left;
    height: 70px;
    margin: 0 9px;
}
.header-right ul.mm-service li a
{
    display: block;
    height: 70px;
    cursor: pointer;
}
.header-right ul.mm-service li:nth-child(1) a
{
    background: url("../images/service_chinhhang_icon.png") no-repeat scroll 0 0 transparent;
    width: 115px;
}
.header-right ul.mm-service li:nth-child(2) a
{
    background: url("../images/service_0_icon.png") no-repeat scroll 0 0 transparent;
    width: 128px;
}
.header-right ul.mm-service li:nth-child(3) a
{
    background: url("../images/service_car_icon.png") no-repeat scroll 0 0 transparent;
    width: 162px;
}

